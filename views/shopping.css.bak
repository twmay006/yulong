body {
    width: 100%;
}

a, b, div, em, h1, h2, h3, h4, h5, h6, li, p, span, strong {
    font-weight: 100;
}
a, body, button, div, h1, h2, h3, h4, h5, h6, img, input, li, ol, p, span, table, td, tr, ul {
    padding: 0;
    margin: 0;
}
.logo {
    left: 0;
    z-index: 1060;
}
.logo, .navbar, .navbar .nav-item {
    float: left;
}
a {
    background-color: transparent;
}
img {
    border: none;
    vertical-align: middle;
}
.header, .header .wrapper, .logo, .navbar .nav-item {
    position: relative;
}
.hide, legend, td, th {
    padding: 0;
}
.hide, .icon {
    position: absolute;
}
h1 {
    font-size: 2em;
}
.cf {
    zoom: 1;
}
li, ol, ul {
    list-style: none;
}
.cf:after, .cf:before {
    content: "";
    display: table;
}
.navbar .nav-item .nav-item-a {
    position: relative;
    display: inline-block;
    vertical-align: top;
    height: 70px;
    line-height: 70px;
    width: 160px;
    border-right: 1px solid #212325;
    text-align: center;
    color: #fcfdfd;
    font-size: 18px;
    background-color: #27292b;
    -webkit-transition: background-color .2s ease;
    transition: background-color .2s ease;
}
.navbar .nav-item .nav-item-a .class {
    margin-top: 26px;
    background-position: -1px -1px;
}
.navbar .nav-item span {
    float: left;
    margin-left: 30px;
    cursor: pointer;
}
.header {
    height: 70px;
    background-color: #27292b;
    z-index: 1060;
}
.header .wrapper {
    width: 990px;
    margin: 0 auto;
    z-index: 10;
}
.hide {
    width: 1px;
    height: 1px;
    margin: -1px;
    overflow: hidden;
    clip: rect(0,0,0,0);
    border: 0;
}
.category .category-bg {
    width: 100%;
    height: 140px;
    background-color: #f7f7f7;
    opacity: .96;
    filter: alpha(opacity=96);
}
.category .category-sub-navbar {
    position: absolute;
    width: 990px;
    left: 50%;
    margin-left: -495px;
    top: 0;
}
.category .category-sub-navbar li {
    margin: 40px 27px 0;
    display: inline-block;
    text-align: center;
}
.category .category-sub-navbar li img {
    width: 50px;
    height: 50px;
}
.category .category-sub-navbar li p {
    color: #3f3f3f;
    font-size: 14px;
}
.navbar .nav-item .nav-item-a .star {
    margin-top: 25px;
    background-position: -22px -1px;
}
.content {
    width: 990px;
    margin: 40px auto 0;
}
.breadcrumb {
    height: 40px;
    line-height: 40px;
    font-size: 0;
}
.breadcrumb>li {
    color: #7b7b7b;
    font-size: 14px;
    -webkit-transition: color .2s ease;
    transition: color .2s ease;
}
.breadcrumb>li>a {
    color: #7b7b7b;
    -webkit-transition: color .2s ease;
    transition: color .2s ease;
}
.breadcrumb>li .diagonal {
    color: #7b7b7b;
    margin: 0 6px;
}
.product-intro {
    margin-bottom: 80px;
}
.product-preview {
    width: 460px;
    padding-top: 6px;
    float: left;
}
.product-preview .product-img, .product-preview .product-img img {
    width: 460px;
    height: 440px;
}
.product-preview .thumbnails {
    margin-top: 20px;
    width: 460px;
    overflow: hidden;
}
.product-preview .thumbnails .thumbnail-list {
    width: 2000px;
}
.product-preview .thumbnails .thumbnail-item {
    float: left;
    margin-right: 20px;
    opacity: .5;
    filter: alpha(opacity=50);
    width: 100px;
    height: 100px;
}
.product-preview .thumbnails .thumbnail-item.active {
    opacity: 1;
    filter: alpha(opacity=100);
}
.product-info .product-price .discount {
    margin-right: 10px;
    display: inline-block;
    width: 20px;
    height: 20px;
    text-align: center;
    border-radius: 2px;
    background-color: #35c367;
    color: #fff;
}
.product-info .product-price .discount-tips {
    color: #35c367;
}
.product-custom .custom {
    overflow: hidden;
    font-size: 0;
}
.quantity-input {
    display: inline-block;
    width: 143px;
    height: 42px;
    position: relative;
    font-size: 16px;
}
.quantity-input .reduce {
    border-top-left-radius: 2px;
    border-bottom-left-radius: 2px;
    left: 0;
}
.quantity-input .quantity-input-tx {
    position: absolute;
    -webkit-appearance: none;
    left: 0;
    width: 40px;
    margin: 0 40px;
    line-height: 22px;
    padding: 10px;
    height: 20px;
    text-align: center;
    border: 1px solid #e4e4e4;
    font-size: 16px;
    background-color: #fff;
}
.quantity-input .add {
    right: 0;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
    border-left: none;
}
.quantity-input .add .icon-add {
    background-position: -1px -188px;
}
.product-info .product-buy .cart {
    margin-right: 15px;
}
.btn-green, .jf-btn-default {
    border: 1px solid #35c367;
    background-color: #35c367;
    -webkit-transition: color .2s ease;
    transition: color .2s ease;
    color: #fff;
}
.btn-red, .jf-btn-secondary-default {
    border: 1px solid tomato;
    background-color: tomato;
    -webkit-transition: color .2s ease;
    transition: color .2s ease;
}
.product-info .product-buy .buy {
    margin: 0;
}
.product-detail {
    margin-bottom: 60px;
}
.product-detail .detail-subnav {
    height: 60px;
    background-color: #fafafa;
}
.product-detail .detail-subnav .subnav-item {
    position: relative;
    float: left;
    margin-left: -1px;
}
subnav-item a{
	font-weight:bold;
}
.product-detail .detail-content .content-item.active {
    display: block;
}
.content-item img{
	margin:0 auto;
}
.product-detail .detail-content .contact_item {
    padding: 30px 50px;
}

.product-detail .detail-content img {
    display: block;
    max-width: 990px;
    vertical-align: top;
}
.fixed-tools {
    position: fixed;
    bottom: 220px;
    left: 50%;
    z-index: 101;
    margin-left: 500px;
    width: 42px;
    text-align: center;
}
.fixed-tools, a {
    text-decoration: none;
}
.tool a {
    position: relative;
    width: 42px;
    padding: 10px 0 10px;
    border: 1px solid #e4e4e4;
    line-height: 1.2;
    color: #ababab;
    font-size: 13px;
    font-weight: 100;
}
.fixed-tools .btn-back-to-top {
    background-position: -1px -223px;
}
.logo img {
    height: 100%;
    width: auto;
}
.logo a {
    display: block;
    height: 76px;
}
.breadcrumb>li>a.active, .breadcrumb>li>a:hover {
    color: #1e1e1e;
}
.breadcrumb>li, .btn, .login .scoring-user, .login .shopping-cart {
    display: inline-block;
}
.product-info {
    margin-left: 500px;
}
.product-info {
    width: 380px;
	height:566px;
}
.product-info .product-name .title {
    font-size: 28px;
    color: #3f3f3f;
	font-weight:bold;
}
.product-info .product-name .txt {
    margin: 15px 0;
    color: #7b7b7b;
    line-height: 20px;
}
.product-info .product-price {
    padding-bottom: 10px;
    border-bottom: 1px solid #ebebeb;
    margin-bottom: 10px;
}
.product-info .product-price p {
    margin-bottom: 10px;
}
.product-info .product-price .current-price {
    margin-right: 10px;
    color: #222;
    font-size: 24px;
    font-weight: 400;
}
.product-info .product-price .old-price {
    font-size: 14px;
    color: #888;
}
.product-price>p>b{
	color:red;
	font-size:20px;
	font-weight:bold;
	margin-left: 10px;
}
.product-info .product-custom, .product-info .product-quantity {
    margin-bottom: 20px;
}
.product-info .product-custom h3, .product-info .product-quantity h3 {
    color: #a3a3a3;
    margin-bottom: 10px;
    font-size: 14px;
}
.product-custom .custom {
    overflow: hidden;
    font-size: 0;
}
.product-custom .custom-item.selected {
    padding: 7px 15px;
    border: 2px solid #35c367;
}
.product-custom .custom-item {
    font-size: 16px;
    display: inline-block;
    padding: 8px 16px;
    margin-right: 20px;
    margin-bottom: 10px;
    color: #7b7b7b;
    font-weight: 400;
    border: 1px solid #d1d1d1;
    border-radius: 2px;
    cursor: pointer;
    text-align: center;
}
.product-info .product-buy {
    position: relative;
    margin-bottom: 20px;
    font-size: 0;
}
.btn-big {
    width: 180px;
    line-height: 48px;
}
.btn {
    text-align: center;
    color: #fff;
    border-radius: 2px;
    font-weight: 200;
}
.btn-green, .jf-btn-default {
    border: 1px solid #35c367;
    background-color: #35c367;
    -webkit-transition: color .2s ease;
    transition: color .2s ease;
    color: #fff;
}
.btn-big, .recommend .section-header .title h2 {
    font-size: 18px;
}
.product-preview .thumbnails .thumbnail-item img {
    width: 100px;
    height: 100px;
}
.product-detail .detail-subnav .subnav-item.active a {
    color: #1e1e1e;
}
.product-detail .detail-subnav .subnav-item a {
    display: block;
    text-align: center;
    line-height: 60px;
    padding: 0 50px;
    color: #6a6a6a;
    font-size: 18px;
}
.fixed-tools .btn-back-to-top {
    background-position: -1px -223px;
}
.fixed-tools {
    position: fixed;
    bottom: 220px;
    left: 50%;
    z-index: 101;
    margin-left: 500px;
    width: 42px;
    text-align: center;
}
.tool, .tools-group {
    margin-bottom: 10px;
}
.tool i {
    position: absolute;
    left: 50%;
    margin-left: -10px;
    top: 11px;
    width: 19px;
    height: 22px;
    background-image: url(images/sprite.2a6126e5.png);
    background-repeat: no-repeat;
}
.fixed-tools .show-mobile-qrcode {
    position: relative;
}
.ico-mobile-store {
    background-position: -150px -372px;
}
.ico-qq-service {
    background-position: -129px -372px;
}
.fixed-tools .btn-back-to-top {
    background-position: -1px -223px;
}
.fixed-tools .btn-back-to-old, .fixed-tools .btn-back-to-top {
    display: block;
    width: 42px;
    height: 42px;
    background-image: url(images/sprite.2a6126e5.png);
    background-repeat: no-repeat;
    border: 1px solid #e4e4e4;
}
.fixed-tools .show-mobile-qrcode .jifen-mobile-qrcode.show, .tool a {
    display: block;
}
.fixed-tools .btn-back-to-top:hover {
    background-position: -1px -267px;
}
.quantity-input .reduce .icon-reduce {
    background-position: -22px -188px;
}
.quantity-input .icon-add, .quantity-input .icon-reduce {
    display: inline-block;
    margin-top: 6px;
    width: 20px;
    height: 20px;
    background-image: url(images/sprite.2a6126e5.png);
    background-repeat: no-repeat;
}
.quantity-input .add .icon-add {
    background-position: -1px -188px;
}
.quantity-input .add, .quantity-input .reduce {
    position: absolute;
    display: block;
    width: 41px;
    height: 42px;
    top: 0;
    text-align: center;
    border: 1px solid #e4e4e4;
    background-color: #fafafa;
    cursor: pointer;
}
.product-detail .detail-content {
    border: 1px solid #f5f5f5;
    border-top: 0;
}
.quantity-input .add:hover, .quantity-input .reduce:hover {
    background-color: #f2f2f2;
}
.login {
    float: right;
    color: #fff;
    font-size: 0;
}
.login .scoring {
    display: inline-block;
    position: relative;
    font-size: 18px;
    cursor: pointer;
}
.login .scoring:hover .scoring-user {
    background-color: #060001;
}
.login .scoring-user {
    position: relative;
    padding: 0 10px;
    color: #fff;
    line-height: 70px;
}
.login .scoring .mine {
    float: left;
    width: 30px;
    height: 30px;
    background-image: url(images/sprite.2a6126e5.png);
    background-position: -1px -22px;
    margin-top: 20px;
}
.login .scoring em {
    float: left;
    margin-left: 10px;
    max-width: 118px;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    cursor: pointer;
}
em {
    font-style: normal;
}
.login .shopping-cart {
    position: relative;
    height: 70px;
    width: 90px;
    vertical-align: top;
}
.login .shopping-cart .cart {
    position: relative;
    margin: 20px auto 0;
    width: 30px;
    height: 30px;
    background-image: url(images/sprite.2a6126e5.png);
    background-position: -32px -22px;
}
.login .shopping-cart .cart .badge {
    position: absolute;
    width: 16px;
    height: 16px;
    right: -9px;
    top: -4px;
    color: #fff;
    font-size: 12px;
    text-align: center;
    line-height: 16px;
    border-radius: 50%;
    background-color: #35c367;
}
.login .shopping-cart:hover {
    background-color: #060001;
}
.fly-item {
    position: absolute;
    top: 0;
    left: 25%;
    margin-left: -10px;
    width: 20px;
    height: 20px;
    background-color: #35c367;
    opacity: .5;
    filter: alpha(opacity=50);
    border-radius: 100%;
    visibility: hidden;
    z-index: 2000;
	transition:transform 1s linear;
}
.fly{
	transform:translate(365px, -510px);
	visibility:visible;
}
.fly-item, .share-to-wechat.show {
    display: block;
}